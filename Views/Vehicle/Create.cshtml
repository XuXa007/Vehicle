@model WebApplication3.Models.Vehicle

<h2>Create Vehicle</h2>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="Make"></label>
        <input asp-for="Make" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Model"></label>
        <input asp-for="Model" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="RegistrationNumber"></label>
        <input asp-for="RegistrationNumber" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>

<a asp-action="VehicleList">Back to List</a>